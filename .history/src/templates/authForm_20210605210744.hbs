<form class='auth-form'>
  <input
    type='email'
    id='email'
    class='swal2-input auth-modal__input'
    placeholder='Email'
    autocomplete=''
  />
  <input
    type='password'
    id='password'
    class='swal2-input auth-modal__input'
    placeholder='Password'
  />
  <p class='auth-modal__text'>
    Don't have account?
    <button class='auth-modal__button' type='button'>
      Create!
    </button>
  </p>
</form>